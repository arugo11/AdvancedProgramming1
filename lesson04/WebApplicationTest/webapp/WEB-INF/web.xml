<?xml version="1.0" encoding="UTF-8"?>

<web-app>

  <servlet>
    <servlet-name>Hello</servlet-name>
    <servlet-class>HelloWorld</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Simple</servlet-name>
    <servlet-class>SimpleServlet</servlet-class>
  </servlet>

  <servlet> 
    <servlet-name>Hello23088</servlet-name>
    <servlet-class>Hello23088</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Plus23088</servlet-name>
    <servlet-class>Plus23088</servlet-class>
  </servlet>



  
  <servlet-mapping>
    <servlet-name>Simple</servlet-name>
    <url-pattern>/servlet/Simple/</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Hello23088</servlet-name>
    <url-pattern>/servlet/hello23088/</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Plus23088</servlet-name>
    <url-pattern>/servlet/plus23088/</url-pattern>
  </servlet-mapping>


</web-app>
